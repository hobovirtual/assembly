    <command:details>
      <command:name>Set-Tls</command:name>
      <command:verb>Set</command:verb>
      <command:noun>Tls</command:noun>
      <maml:description>
        <maml:para>Sets the TLS settings with only those supplied.</maml:para>
      </maml:description>
